<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/orderNumberTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:fontFamily="@font/calibri_regular"
        android:textAlignment="center"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/descriptionTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/descriptionTextView"
        tools:text="99" />

    <TextView
        android:id="@+id/descriptionTextView"
        android:layout_width="300dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/calibri_regular"
        android:lineSpacingExtra="8sp"
        android:textSize="24sp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/orderNumberTextView"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="TextView" />

    <TextView
        android:id="@+id/typeQuestionTextView"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/calibri_regular"
        android:lineSpacingExtra="8sp"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/descriptionTextView"
        app:layout_constraintStart_toStartOf="@+id/descriptionTextView"
        app:layout_constraintTop_toBottomOf="@+id/descriptionTextView"
        tools:text="Type Question" />

    <TextView
        android:id="@+id/descriptionTextView4"
        android:layout_width="62dp"
        android:layout_height="21dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/calibri_regular"
        android:lineSpacingExtra="8sp"
        android:text="0%"
        android:textAlignment="center"
        android:textColor="@color/red"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/descriptionTextView2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/descriptionTextView3"
        app:layout_constraintTop_toBottomOf="@+id/typeQuestionTextView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/descriptionTextView2"
        android:layout_width="62dp"
        android:layout_height="21dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/calibri_regular"
        android:lineSpacingExtra="8sp"
        android:text="0%"
        android:textAlignment="center"
        android:textColor="#FFC107"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/descriptionTextView5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/descriptionTextView4"
        app:layout_constraintTop_toBottomOf="@+id/typeQuestionTextView"
        app:layout_constraintVertical_bias="0.055" />

    <TextView
        android:id="@+id/descriptionTextView5"
        android:layout_width="62dp"
        android:layout_height="21dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/calibri_regular"
        android:lineSpacingExtra="8sp"
        android:text="0%"
        android:textAlignment="center"
        android:textColor="#2196F3"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/descriptionTextView2"
        app:layout_constraintTop_toBottomOf="@+id/typeQuestionTextView"
        app:layout_constraintVertical_bias="0.055" />

    <TextView
        android:id="@+id/descriptionTextView3"
        android:layout_width="66dp"
        android:layout_height="22dp"
        android:layout_marginStart="@dimen/actionBarSize"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/calibri_regular"
        android:lineSpacingExtra="8sp"
        android:text="0%"
        android:textAlignment="center"
        android:textColor="@color/green_light"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/descriptionTextView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="@+id/typeQuestionTextView"
        app:layout_constraintTop_toBottomOf="@+id/typeQuestionTextView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="8dp"
        android:alpha="0.7"
        android:background="@color/khaky"
        android:textColor="@color/darkGray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/descriptionTextView3" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginStart="8dp"
        android:alpha="0.7"
        android:background="@color/khaky"
        android:textColor="@color/darkGray"
        app:layout_constraintBottom_toTopOf="@+id/descriptionTextView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/typeQuestionTextView" />

</androidx.constraintlayout.widget.ConstraintLayout>